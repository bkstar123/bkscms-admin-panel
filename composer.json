{
    "name": "bkstar123/bkscms-admin-panel",
    "description": "A bkstar123/bkscms's package for authentication and authorization of admin users",
    "keywords": [
        "laravel",
        "admin",
        "admin-auth",
        "admin-authentication",
        "admin-authorization",
        "authentication",
        "authorization"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hoang Anh Tuan",
            "email": "hoangtuandhbk@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.3",
        "bkstar123/flashing": "^1.0",
        "bkstar123/laravel-recaptcha": "^1.1",
        "bkstar123/mysql-search": "^1.0",
        "bkstar123/bkscms-utilities": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Bkstar123\\BksCMS\\AdminPanel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bkstar123\\BksCMS\\AdminPanel\\Providers\\AdminPanelServiceProvider"
            ]
        }
    }
}
